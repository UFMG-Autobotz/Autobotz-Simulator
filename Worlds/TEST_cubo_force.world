<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">

    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <model name="box">
      <include>
        <uri>model://TEST_box/</uri>
      </include>

      <plugin name="apply_force" filename="libapply_force.so">
        <force>0.05</force>
      </plugin>

    </model>

  </world>
</sdf>
