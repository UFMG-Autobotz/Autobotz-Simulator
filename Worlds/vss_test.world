<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">

    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <model name="player">
      <include>
        <uri>model://VSS_Player/</uri>
      </include>

      <plugin name="player plugin" filename="libpid_control_plugin.so">
        <velocity>true</velocity>
        <pid>0.001 0 0</pid>
      </plugin>
    </model>

    <model name="ball">
      <include>
        <uri>model://VSS_Ball/</uri>
        <pose>0.2 0 0 0 0 0</pose>
      </include>
    </model>

    <model name="camera">
      <include>
        <uri>model://camera_ros</uri>
        <pose>0 0. 2 0 1.570793 0</pose>
        <static>true</static>
      </include>
    </model>

  </world>
</sdf>
