<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">

    <plugin name="config" filename="libPlayers_Config.so">
      <param1 model="player1">VSS_Player/yellow1</param1>
      <param2 model="player2">VSS_Player/blue2</param2>
    </plugin>

    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <model name="player1">
      <include>
        <uri>model://VSS_Player/</uri>
        <pose>-0.2 0 0 0 0 0</pose>
      </include>

      <plugin name="player 1 plugin" filename="libpid_control_plugin.so">
        <velocity>true</velocity>
        <pid>0.001 0 0</pid>
      </plugin>
    </model>

    <model name="player2">
      <include>
        <uri>model://VSS_Player/</uri>
        <pose>0.2 0 0 0 0 3.14</pose>
      </include>

      <plugin name="player2 plugin" filename="libpid_control_plugin.so">
        <velocity>true</velocity>
        <pid>0.001 0 0</pid>
      </plugin>
    </model>

    <model name="ball">
      <include>
        <uri>model://VSS_Ball/</uri>
      </include>
    </model>

    <model name="camera">
      <include>
        <uri>model://camera_ros</uri>
        <pose>0 0. 2 0 1.570793 0</pose>
        <static>true</static>
      </include>
    </model>

  </world>
</sdf>
